<?xml version="1.0" encoding="utf-8" ?>
<config>
    <listen-folder>
        <!--
            例子：LF00RT001、LF00RT002
            LF: listen folder
            RT: root
        -->
        <item id="LF00RT001" alias="临时资源库">/Volumes/Repository/临时</item>
        <item id="LF00RT003" alias="本机文档">/Users/yuanjin/Documents</item>
    </listen-folder>

<!--保护的文件：规则配置-->
    <protect-file>
        <path>
            <item>README.md</item>
            <item>Parallels虚拟机(Mac下融合运行WIN超好用，已破解购买).dmg</item>
        </path>
        <file-path-regex>
<!--            <item>^\d{3}_.*$</item>-->
<!--            <item>^/Users.*$</item>-->
            <item>^LF00RT003.*$</item>
        </file-path-regex>
    </protect-file>
</config>